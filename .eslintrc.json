{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
     "rules": {
        "prop-types": [
            0
        ],
        "no-duplicate-imports": "warn",
        "constructor-super": "warn",
        "camelcase": "warn",
        "complexity": "warn",
        "eqeqeq": "error",
        "semi-style": "error"
    }
}